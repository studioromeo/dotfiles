"*":
  "autocomplete-plus":
    autoActivationDelay: 200
    confirmCompletion: "enter"
    maxVisibleSuggestions: 6
    suggestionListFollows: "Cursor"
  "bracket-matcher":
    autocompleteBrackets: false
    autocompleteSmartQuotes: false
  core:
    disabledPackages: [
      "archive-view"
      "welcome"
      "php-cs-fixer"
      "svg-preview"
      "metrics"
      "language-vue-component"
      "editorconfig"
      "about"
      "autocomplete-atom-api"
      "autosave"
      "background-tips"
      "bookmarks"
      "deprecation-cop"
      "dev-live-reload"
      "exception-reporting"
      "grammar-selector"
      "incompatible-packages"
      "keybinding-resolver"
      "link"
      "notifications"
      "open-on-github"
      "package-generator"
      "spell-check"
      "styleguide"
      "update-package-dependencies"
      "language-gherkin"
      "symbols-view"
      "line-ending-selector"
      "encoding-selector"
      "language-javascript-jsx"
      "pigments"
